<?xml version="1.0"?>
<project name="Example">

    <!--Timeline Properties-->
    <timeline>
        <settings ticksPerSecond="20" secondsPerPixel="0.005" leftTickCutoff="0" verticalPixelScroll="0" trackHeight="60" />

        <track name="Translation">
            <clip movementName="LinearTranslation" startTick="20" endTick="90" minDuration="5.0"/>
            <clip movementName="LinearTranslation" startTick="90" endTick="100" minDuration="5.0"/>
            <clip movementName="LinearTranslation" startTick="110" endTick="200" minDuration="5.0"/>
        </track>

        <track name="Rotation">
            <clip movementName="LinearRotation" startTick="40" endTick="90" minDuration="5.0"/>
            <clip movementName="LinearRotation" startTick="150" endTick="300" minDuration="5.0"/>
        </track>

        <track name="Lift">
            <clip movementName="LinearLift" startTick="0" endTick="1000" minDuration="5.0"/>
        </track>

    </timeline>

</project>
