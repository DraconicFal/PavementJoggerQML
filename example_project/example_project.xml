<?xml version="1.0"?>
<project name="Example">



    <!--Palette Items-->
    <palette>

        <subsystem name="Translation">
            <movement movementName="LinearTranslation" />
        </subsystem>

        <subsystem name="Rotation">
            <movement movementName="LinearRotation" />
        </subsystem>

        <subsystem name="Lift">
            <movement movementName="LinearLift" />
        </subsystem>

    </palette>



    <!--Timeline Items-->
    <timeline>
        <settings ticksPerSecond="20" secondsPerPixel="0.005" timelineHeight="300" leftTickCutoff="0" verticalPixelScroll="0" trackHeight="60" />

        <track name="Translation">
            <clip movementName="LinearTranslation" startTick="40" endTick="300" minDuration="0.0"/>
            <clip movementName="LinearTranslation" startTick="300" endTick="420" minDuration="60.0"/>
            <clip movementName="LinearTranslation" startTick="580" endTick="840" minDuration="60.0"/>
        </track>

        <track name="Rotation">
            <clip movementName="LinearRotation" startTick="0" endTick="300" minDuration="60.0"/>
            <clip movementName="LinearRotation" startTick="360" endTick="720" minDuration="60.0"/>
        </track>

        <track name="Lift">
            <clip movementName="LinearLift" startTick="360" endTick="480" minDuration="60.0"/>
            <clip movementName="LinearLift" startTick="580" endTick="700" minDuration="60.0"/>
        </track>

    </timeline>

</project>
